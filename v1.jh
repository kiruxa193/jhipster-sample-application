
entity User {
  login String required unique,
  password String required,
  firstName String,
  lastName String,
  email String required unique,
  // Добавьте дополнительные поля по вашему усмотрению
}

entity Friend {
  name String required,
  friendMail String required unique,
  congratulation String 
  
  // Добавьте дополнительные поля по вашему усмотрению
}

relationship ManyToMany {
  User{friends(name)} to Friend,
}





